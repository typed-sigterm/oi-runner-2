{
  "publisher": "typed-sigterm",
  "name": "oi-runner-2",
  "displayName": "OI Runner++",
  "version": "1.3.2",
  "private": true,
  "packageManager": "pnpm@10.8.0",
  "description": "VS Code extension designed for OIer and ACMer, for running single-file programs.",
  "license": "MIT",
  "homepage": "https://oi-runner-2.by-ts.top",
  "repository": {
    "url": "https://github.com/typed-sigterm/oi-runner-2"
  },
  "categories": [
    "Debuggers",
    "Other"
  ],
  "main": "dist/extension/index.js",
  "icon": "assets/logo.png",
  "engines": {
    "node": "22",
    "vscode": "^1.94.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "oi-runner-2.runCode",
        "title": "Launch OI Runner++",
        "icon": "assets/logo.png"
      }
    ],
    "menus": {
      "editor/title/run": [
        {
          "command": "oi-runner-2.runCode",
          "group": "z_commands",
          "when": "config.oi-runner-2.addToRunMenu"
        }
      ]
    },
    "configuration": [
      {
        "title": "OI Runner++",
        "properties": {
          "oi-runner-2.tasks": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "type": "object",
            "patternProperties": {
              ".*": {
                "type": "object",
                "propertyNames": [
                  "compile",
                  "execute"
                ],
                "patternProperties": {
                  "^compile|execute$": {
                    "type": "array",
                    "items": [
                      {
                        "type": "string",
                        "description": "The command to execute."
                      },
                      {
                        "type": "array",
                        "items": {
                          "type": "string"
                        },
                        "description": "The arguments to pass to the command."
                      }
                    ],
                    "minLength": 2,
                    "maxLength": 2
                  }
                },
                "required": [
                  "execute"
                ],
                "additionalProperties": false
              }
            },
            "default": {
              "C++": {
                "compile": [
                  "g++",
                  [
                    "${file}",
                    "-o${fileNoExt}",
                    "-std=c++14",
                    "-O2"
                  ]
                ],
                "execute": [
                  "${fileNoExt}${execExt}",
                  []
                ]
              },
              "Python": {
                "execute": [
                  "python",
                  [
                    "${file}"
                  ]
                ]
              }
            },
            "description": "The tasks available in the run panel and their corresponding compilation and execution commands."
          },
          "oi-runner-2.defaultTask": {
            "type": "object",
            "patternProperties": {
              "^\\.[^.]+$": {
                "type": "string",
                "description": "The default task for a specific file extension."
              }
            },
            "additionalProperties": false,
            "default": {
              ".cpp": "C++",
              ".py": "Python"
            }
          },
          "oi-runner-2.addToRunMenu": {
            "type": "boolean",
            "default": true,
            "description": "Whether to add OI Runner++ to the editor title menu."
          },
          "oi-runner-2.autoSave": {
            "type": "boolean",
            "default": true,
            "description": "Whether to automatically save the file before running."
          },
          "oi-runner-2.textareaMaxSize": {
            "type": "integer",
            "default": 1e4,
            "description": "The maximum size of the textarea for stdout in bytes in utf-8 encoding."
          }
        }
      }
    ],
    "viewsContainers": {
      "panel": [
        {
          "id": "oi-runner-2",
          "title": "OI Runner++",
          "icon": "./assets/logo.png"
        }
      ]
    },
    "views": {
      "oi-runner-2": [
        {
          "type": "webview",
          "id": "oi-runner-2.panel",
          "name": "OI Runner++",
          "icon": "./assets/logo.png"
        }
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "build": "vue-tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepare": "husky",
    "test:types": "vue-tsc -p ./tsconfig.app.json --noEmit && vue-tsc -p ./tsconfig.node.json --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.3.0",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@guolao/vue-monaco-editor": "^1.5.4",
    "@iconify-prerendered/vue-codicon": "^0.28.1740377626",
    "@iconify-prerendered/vue-line-md": "^0.28.1736493333",
    "@tomjs/vite-plugin-vscode": "^4.0.0",
    "@typed-sigterm/eslint-config": "^1.4.1",
    "@types/node": "^22.13.8",
    "@types/ps-tree": "^1.1.6",
    "@types/vscode": "^1.97.0",
    "@types/vscode-webview": "^1.57.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vscode-elements/elements": "^1.13.1",
    "@vscode/vsce": "^3.2.2",
    "consola": "^3.4.0",
    "eslint": "^9.21.0",
    "husky": "^9.1.7",
    "monaco-editor": "^0.52.2",
    "ps-tree": "^1.2.0",
    "tree-kill": "^1.2.2",
    "typescript": "^5.8.2",
    "vite": "^6.2.0",
    "vitepress": "^1.6.3",
    "vitepress-giscus": "^1.0.2",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.6",
    "zod": "^3.24.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@vscode/vsce-sign",
      "esbuild",
      "keytar",
      "vue-demi"
    ]
  }
}
