{
  "publisher": "typed-sigterm",
  "name": "oi-runner-2",
  "displayName": "OI Runner++",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.12.2",
  "description": "VS Code extension designed for OIer and ACMer, for running single-file programs.",
  "license": "MIT",
  "homepage": "https://oier-runner-2.by-ts.top",
  "repository": {
    "url": "https://github.com/typed-sigterm/oi-runner-2"
  },
  "categories": [
    "Debuggers",
    "Other"
  ],
  "main": "dist/extension/index.js",
  "icon": "assets/logo.png",
  "engines": {
    "node": "20",
    "vscode": "^1.94.0"
  },
  "contributes": {
    "configuration": [
      {
        "title": "OI Runner++",
        "properties": {
          "oi-runner-2.tasks": {
            "type": "object",
            "default": {
              "C++": {
                "compile": [
                  "g++",
                  [
                    "${file}",
                    "-o${fileNoExt}",
                    "-std=c++14",
                    "-O2"
                  ]
                ],
                "execute": [
                  "${fileNoExt}${execExt}",
                  []
                ]
              },
              "Python": {
                "execute": [
                  "python",
                  [
                    "${file}"
                  ]
                ]
              }
            },
            "description": "Commands to run when compile or run."
          }
        }
      }
    ],
    "viewsContainers": {
      "panel": [
        {
          "id": "oi-runner-2",
          "title": "OI Runner++",
          "icon": "assets/logo.png"
        }
      ]
    },
    "views": {
      "oi-runner-2": [
        {
          "type": "webview",
          "id": "oi-runner-2.panel",
          "name": "OI Runner++"
        }
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "build": "vue-tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "@codemirror/view": "^6.34.1",
    "@iconify-prerendered/vue-codicon": "^0.25.1727504422",
    "@vscode-elements/elements": "^1.6.2",
    "codemirror": "^6.0.1",
    "consola": "^3.2.3",
    "convert": "^5.4.1",
    "iconv-lite": "^0.6.3",
    "vue": "^3.5.10",
    "vue-codemirror6": "^1.3.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.3",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@tomjs/vite-plugin-vscode": "^3.0.0",
    "@typed-sigterm/eslint-config": "^1.3.0",
    "@types/vscode": "^1.94.0",
    "@types/vscode-webview": "^1.57.5",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vscode/vsce": "^3.2.0",
    "eslint": "^9.12.0",
    "husky": "^9.1.6",
    "typescript": "^5.5.3",
    "vite": "^5.4.8",
    "vitepress": "^1.4.1",
    "vitepress-plugin-comment-with-giscus": "^1.1.15",
    "vue-tsc": "^2.1.6"
  }
}
